<!DOCTYPE html>
<html>
<head>
  <title><%= title%></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="container">
		<%= render 'layouts/header' %>
		<section class="white">
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>"><%= value %></div>
			<% end %>
			<%= yield %>
		</section>
    </div>
</body>
</html>
